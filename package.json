{
  "name": "rankverse-backend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:alvIndieDevelop/rankverse-backend.git",
  "author": "Alvaro Martin Caballero <alvindiedevelop@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "npm run serve",
    "serve": "node build/index.js",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "build": "tsc -p .",
    "tsc": "tsc"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ignore": [
      "node_modules"
    ],
    "ext": "ts",
    "exec": "ts-node --transpileOnly src/index.ts"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "helmet": "^8.0.0",
    "jwt-simple": "^0.5.6",
    "moment": "^2.30.1",
    "mongoose": "^8.9.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "serve": "^14.2.4",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/helmet": "^4.0.0",
    "@types/jwt-simple": "^0.5.36",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.2",
    "@types/node-cron": "^3.0.11",
    "concurrently": "^9.1.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
